<% if current_user.evaluating?(ramenpost)%>
  <%= form_for(current_user.eavaluations.find_by(ramenpost_id: ramenpost.id), html: { method: :delete }) do |f| %>
    <%= hidden_field_tag :ramenpost_id, ramenpost.id %>
     <div id="demo"></div>
     <script>$('star').raty();</script>
  <% end %>
<% else %>
  <%= form_for(current_user.eavaluations.build) do |f| %>
    <%= hidden_field_tag :ramenpost_id, ramenpost.id %>
　　<div id="star"></div>
　  <script>$('srar').raty();</script>  <% end %>
<% end %>

        <script>
          $('#star').raty({
          size     : 36,
          starOff:  '<%= asset_path('star-off.png') %>',
          starOn : '<%= asset_path('star-on.png') %>',
          scoreName: 'review[star]' 
           });
        </script>